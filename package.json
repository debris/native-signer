{
  "name": "NativeSigner",
  "version": "3.0.0-beta",
  "private": true,
  "license": "GPL-3.0",
  "engines": {
    "node": ">=8.3",
    "yarn": "^1.3.2"
  },
  "scripts": {
    "build-rust-ios": "cd rust/signer && make ios",
    "build-rust-android": "cd rust/signer && make android",
    "ios": "yarn run build-rust-ios && react-native run-ios",
    "android": "yarn run build-rust-android && react-native run-android --appIdSuffix \"dev\"",
    "start": "yarn run ios",
    "postinstall": "npx jetify &&  npx rn-nodeify --install --hack",
    "test": "jest",
    "lint": "eslint",
    "commit": "commit-wizard"
  },
  "dependencies": {
    "@polkadot/reactnative-identicon": "^0.42.0-beta.25",
    "@polkadot/ui-shared": "^0.42.0-beta.24",
