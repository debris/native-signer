    "@react-native-community/netinfo": "^4.1.1",
    "@tradle/react-native-http": "^2.0.1",
    "bignumber.js": "^4.0.0",
    "browserify-zlib": "^0.1.4",
    "buffer": "^4.9.1",
    "dns.js": "^1.0.1",
    "hoist-non-react-statics": "^2.5.0",
    "https-browserify": "0.0.1",
